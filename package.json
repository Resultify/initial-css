  ],
  "type": "module",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint-css": "stylelint 'src/**/*.css'",
    "standardJs": "standard -v 'build/**/*.js'",
    "checkJs": "tsc -p jsconfig.json --maxNodeModuleJsDepth 0",
    "test": "npm run lint-css && npm run standardJs && npm run checkJs",
    "build": "node ./build/build.exec.js",
    "watch": "node ./build/watch.exec.js",
    "start": "npm run watch & http-server -p 8083 demo/"
  },
  "devDependencies": {
    "@types/node": "~20.2.5",
    "@types/postcss-import": "~14.0.0",
    "@types/semver": "~7.5.0",
    "chalk": "~5.2.0",
    "chokidar": "~3.5.3",
    "cssnano": "~6.0.1",
    "del": "~7.0.0",
    "globby": "^13.1.4",
    "http-server": "~14.1.1",
    "husky": "~8.0.1",
    "postcss-import": "~15.1.0",
    "semver": "~7.5.0",
    "standard": "~17.1.0",
    "stylelint": "~15.6.1",
    "stylelint-config-recommended": "~12.0.0",
    "stylelint-no-unsupported-browser-features": "~6.1.0",
    "typescript": "~5.0.4"
  },
  "engines": {
    "node": ">=16.14",
    "npm": ">=8.19"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "ignore": [
      "docs/"
    ]
  },
  "browserslist": [
    "last 3 iOS major versions and > 0.5%",
    "last 3 Safari major versions and > 0.5%",
    "last 3 Chrome major versions and > 0.5%",
    "last 3 Edge major versions and > 0.5%",
    "last 3 Firefox major versions and > 0.5%",
    "last 3 ChromeAndroid major versions and > 0.5%",
    "> 1%"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-recommended"
    ],
    "plugins": [
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {},
    "ignoreFiles": [
      "docs/"
    ]
  }
